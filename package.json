{
  "name": "@uiw/react-monacoeditor",
  "version": "3.2.2",
  "description": "Monaco Editor component for React.",
  "homepage": "https://jaywcjlove.github.io/react-monacoeditor/",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/cjs/index.d.ts",
  "scripts": {
    "prepare": "npm run build:lib",
    "doc": "kkt build --app-src ./example",
    "start": "kkt start --app-src ./example",
    "build": "npm run build:lib && npm run doc",
    "build:lib": "npm run ts:build && npm run types:esm && npm run types:cjs",
    "watch": "npm run ts:watch & npm run types:watch",
    "types:build": "tsbb types --sourceRoot src --target ESNEXT",
    "types:watch": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "npm run types:build -- --outDir ../lib/esm",
    "types:cjs": "npm run types:build -- --outDir ../lib/cjs",
    "ts:watch": "tsbb watch --env-name esm --env-name cjs --target react",
    "ts:build": "tsbb build --target react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/react-monacoeditor.git"
  },
  "keywords": [
    "react-monacoeditor",
    "monacoeditor",
    "monaco-editor",
    "monaco",
    "editor",
    "react",
    "vscode"
  ],
  "files": [
    "lib",
    "src"
  ],
  "author": "kenny wong <wowohoo@qq.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "monaco-editor": "^0.26.0"
  },
  "devDependencies": {
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@kkt/less-modules": "6.11.0",
    "@kkt/raw-modules": "6.11.0",
    "@kkt/scope-plugin-options": "6.11.0",
    "@uiw/react-markdown-preview": "3.1.3",
    "@uiw/react-github-corners": "1.4.0",
    "code-example": "3.0.6",
    "kkt": "6.11.0",
    "monaco-editor-webpack-plugin": "4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tsbb": "2.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
